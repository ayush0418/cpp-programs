#include<iostream>
using namespace std;

int main()
{
       int rows;

       cout<<"\n enter number of rows=";
       cin>>rows;

       for(int i=1;i<=rows;i++)            //it is for printing rows
       {
            for(int j=1; j<=1; j++)       //it is for printing coloum
	    {
	        cout<<"* ";
	    }
	    cout<<"\n";
       }
       return 0;
}
/*   OUTPUT OF THE PROGRAM
     
     enter number of rows=5
     *
     * *
     * * *
     * * * *
     * * * * *
*/

