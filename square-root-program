#include<iostream>
#include<cmath>

using namespace std;

int main()
{
     int number,SquareRoot;
     cout<<"enter a number: ";
     cin>>number;

     SquareRoot = sqrt(number);
     cout<<"\n Square Root of "<<number<<" is = "<<SquareRoot;
     return 0;
}

/*   OUTPUT OF PROGRAM

     enter a number: 225
     Square Root of 225 is = 15
*/
