#include<iostream>
using namespace std;

int main()
{
       
       int n;

       cout<<"enter a positive integer: "
       cin>>n;

       for(int i=1;i<=n; i++)
       {
              cout<< n << " * "<< i <<" = "<< n*i;
       }

       return 0;
}


/*    OUTPUT OF PROGRAM
      
      enter a positive integer: 6
      6 * 1 = 6
      6 * 2 = 12
      6 * 3 = 18
      6 * 4 = 24
      6 * 5 = 30
      6 * 6 = 36
      6 * 7 = 42
      6 * 8 = 48
      6 * 9 = 54
      6 * 10 = 60
*/

