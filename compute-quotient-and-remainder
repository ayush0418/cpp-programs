#include<iostream>
using namespace std;
int main()
{
  int divisor,dividend,quotient,remainder;
  cout<<"\n enter dividend";
  cin>>dividend;

  cout<<"\n enter divisor";
  cin>>divisor;
  quotient = (dividend / divisior);  
  remainder = (dividend % divisor);
  cout<<"Quotient ="<<quotient;
  cout<<"Remaninder ="<<remainder;

  return 0;
}
