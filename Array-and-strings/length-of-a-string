#include <iostream>
#include <cstring>
using namespace std;

int main()
{
    char str[] = "Ayush Bansal";

    // you can also use str.lemgth()
    // you can also use str.size()
    cout << "String Length = " << strlen(str);

    return 0;
}

/*  OUTPUT OF THE PROGRAM
    
    String Length = 11
*/

