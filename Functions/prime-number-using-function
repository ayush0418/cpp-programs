#include<iostream>
using namespace std;

int checkPrimeNo(int);

int main()
{
      int n;

      cout<<"\n Enter a positive integer";
      cin>> n;

      if(checkPrimeNo(n) == 0)
          cout<< n <<" is a prime number. ";
      else
          cout<< n <<" is not a prime number, ";
      return 0;
}
