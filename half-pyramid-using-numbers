#include<iostream>
using namespace std;

int main()
{
      int rows;
      cout<<"\n enter number of rows";
      cin>>rows;
      
      for(int i=1;i<=rows;i++)               //it will print the rows
      {
           for(int j=1;j<=i;j++)              //it will print the coloum 
	   {
	   cout<<j<<" ";                        //" " this will give the space b/w the numbers in the rows
	   }
      cout<<"\n";
      }
      return 0;
}
