#include<iostream>
#include<math.h>
using namespace std:
int main()
{

float a,b,c,x1,x2,d;
cout<<"\n enter coefficient a,b and c";
cin>>a>>b>>c;
d = b*b -4*a*c;

if (d>0)
x1 = (-b + sqrt(d))/(2*a)
x2 = (-b + sqrt(d))/(2*a)
cout<<"x1 = "<<x1;
cout<<"x2 = "<<x2;

else if (d==0)
cout<<"both the roots are equal";
x1 = -b/(2*a);
cout<<"x1 = x2 = "<<x1;
}
